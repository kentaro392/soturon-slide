	journal = "Economic Design",
	year	= "1996",
	volume	="1",
	pages	="343--354"
}

%%%%% inproceedings %%%%%%%%%%%%%%%%%%%
%研究録や国際会議の論文参照に使う
%    必須フィールド -- author, booktitle, title, year
%    任意フィールド -- (volume または number), address, editor, month, note, organization, pages, publisher, series
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

@inproceedings{Du3,
	author = {D. Duverney and I. Shiokawa},
